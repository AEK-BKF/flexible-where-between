{
    "name": "primitive/flexible-where-between",
    "description": "",
    "keywords": [
        "primitive",
        "flexible-where-between"
    ],
    "homepage": "https://github.com/primitivesocial/flexible-where-between",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jerred Hurst",
            "email": "jerred@leadwithprimitive.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Primitive\\FlexibleWhereBetween\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Primitive\\FlexibleWhereBetween\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Primitive\\FlexibleWhereBetween\\FlexibleWhereBetweenServiceProvider"
            ],
            "aliases": {
                "FlexibleWhereBetween": "Primitive\\FlexibleWhereBetween\\FlexibleWhereBetweenFacade"
            }
        }
    }
}
